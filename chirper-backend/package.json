{
    "name": "chirper-backend",
    "version": "1.0.0",
    "description": "Backend for chirper.",
    "main": "index.js",
    "scripts": {
        "build": "tsc --build && tsc-alias",
        "start": "node dist/src/index.js",
        "dev": "nodemon src/index.ts",
        "seed": "npx knex seed:run",
        "test": "echo \"Error: no test specified\" && exit 1",
        "migrate:up": "knex migrate:up",
        "migrate:down": "knex migrate:down",
        "build:lambda": "tsc --build && tsc-alias && npm run package:lambda",
        "package:lambda": "package.cmd"
    },
    "author": "marutsuki",
    "license": "MIT",
    "dependencies": {
        "aws-lambda": "^1.0.7",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.3",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.4.7",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "knex": "^3.1.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.13.1",
        "pino": "^8.18.0",
        "pino-http": "^9.0.0",
        "serverless-http": "^3.2.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.148",
        "@types/bcrypt": "^5.0.2",
        "@types/body-parser": "^1.19.5",
        "@types/cookie-parser": "^1.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.11.16",
        "@types/passport": "^1.0.16",
        "@types/passport-jwt": "^4.0.1",
        "@types/pg": "^8.11.10",
        "nodemon": "^3.0.3",
        "pino-pretty": "^10.3.1",
        "sqlite3": "^5.1.7",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.8",
        "typescript": "^5.2.2",
        "tsconfig-paths": "^4.2.0"
    }
}
